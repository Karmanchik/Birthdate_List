<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="app.nocamelstyle.birthdatelist.ui.createcontainer.CreateContainerFragment"
        tools:layout="@layout/fragment_create_event"
        android:label="@string/title_home" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="app.nocamelstyle.birthdatelist.ui.fulllist.AllEventsFragment"
        tools:layout="@layout/fragment_all_events"
        android:label="@string/title_dashboard"/>

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="app.nocamelstyle.birthdatelist.ui.fastlist.FastEventsFragment"
        tools:layout="@layout/fragment_fast_events"
        android:label="@string/title_notifications" />
</navigation>